	<queryString>
		<![CDATA[select p.fechaproduccion::DATE fecha_produccion,
lp.idturno turno,lpe.idlinea linea, emp.codigoevolution codigo,
emp.nombres, audit.fechareg fecha_movimiento, lmm.descripcion
FROM
limpieza_proceso_empleado lpe
inner join limpieza_proceso_empleado_aditoria_movimiento audit
on audit.idempleado=lpe.idempleado and audit.idlimpiezaproceso=lpe.idlimpiezaproceso
INNER JOIN limpieza_motivo_movimiento lmm on lmm.idlimpiezamotivomovimiento=audit.idlimpiezamotivomovimiento
INNER JOIN empleado emp on emp.idempleado=lpe.idempleado
INNER JOIN limpieza_proceso lp on lp.idlimpiezaproceso=lpe.idlimpiezaproceso
INNER JOIN produccion p on p.idproduccion=lp.idproduccion
where p.idproduccion>=$P{fecha1} and p.idproduccion>=$P{fecha2}]]>
	</queryString>
