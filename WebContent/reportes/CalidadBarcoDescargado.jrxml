	<queryString>
		<![CDATA[select

         (select bdl.descripcion from barco_descarga_lugar bdl where bdl.idbarcodescargalugar = bd.idbarcodescargalugar)as lugar_descarga,
         (select bdmt.descripcion from barco_descarga_medio_transporte bdmt where bdmt.idbarcodescargamediotransporte = bd.idbarcodescargamediotransporte)as recibido_en,
    	(case when b.idpropiedad = 5 then
    	b.nombre || ' (' || (select descripcionmercante from  mercante where idmercante = (select idmercante from barco_descarga where idbarcodescarga = bd.idbarcodescarga)) || ')'
    	else
    	b.nombre
    	end) as barco,
    	b.numbarco, det.nummuestreo, ltrim(mues.descripcion,'#') as muestreo,

    		case when det.lotecamara is null THEN
    		''
    		else
    		det.lotecamara
    		end lotecamara,


    		(select 'L' || array_to_string(array_agg(haccp ), ', ') haccp
    		from (select DISTINCT (cc.lotehaccp) haccp
    		from camara_cajon cc
    		inner join barco_estiba_cuba_detalle_mezcla becdm
    		on cc.idcamaracajon = becdm.idcamaracajon
    		inner join barco_estiba_cuba bec
    		on becdm.idbarcoestibacuba = bec.idbarcoestibacuba
    		inner join calidad_detalle_pcc cdpp
    		on becdm.idbarcoestibacuba = cdpp.idbarcoestibacuba
    		where cc.idbarcodescarga = bd.idbarcodescarga and cdpp.lotecamara = cc.lotecamara and  cc.lotecamara = det.lotecamara) t1) haccp,

    		(select  array_to_string(array_agg(haccp ), ', ') haccp
    		from (select DISTINCT (cc.lotehaccp) haccp
    		from camara_cajon cc
    		inner join barco_estiba_cuba_detalle_mezcla becdm
    		on cc.idcamaracajon = becdm.idcamaracajon
    		inner join barco_estiba_cuba bec
    		on becdm.idbarcoestibacuba = bec.idbarcoestibacuba
    		inner join calidad_detalle_pcc cdpp
    		on becdm.idbarcoestibacuba = cdpp.idbarcoestibacuba
    		where cc.idbarcodescarga = bd.idbarcodescarga and cdpp.lotecamara = cc.lotecamara and  cc.lotecamara = det.lotecamara) t1) haccp2,


   	bd.fechadescarga fecha_descarga,
    	det.fechareg as fecha_inicio_descarga,
    	to_char (det.fechareg, 'DD.MM.YYYY') as fecha, to_char (det.fechareg, 'HH24:MI:SS') as hora,
    	bd.registro, cp.idcalidadpcc, cu.nombrecuba, es.nombre as especie, es.codigoqm as especieqm, tal.nombre as talla,

    	(case when (tal.idtalla < 10) THEN '0' || tal.idtalla::text else tal.idtalla::text end ) as idtalla,
    	(CASE WHEN det.temperatura isnull or det.temperatura = -1999.91 THEN '0'
    	ELSE round(det.temperatura::numeric,1)::double precision
    	END) temperatura,

    	(CASE WHEN det.sal isnull or det.sal = -1999.91 THEN  '0'
    	ELSE round(det.sal::numeric,2)::double precision
    	END) sal,

    	(CASE WHEN det.histamina isnull or det.histamina = -1999.91 THEN '0'
    	ELSE
    	 round((det.histamina/10)::numeric,2)::double precision
    	END) histamina,

        (CASE WHEN det.histamina isnull or det.histamina = -1999.91 THEN '0'
    	ELSE
    	 round((det.histamina)::numeric,2)::double precision
    	END) histaminappm,

    	(CASE WHEN det.nitrogeno isnull  or det.nitrogeno = -1999.91 THEN '0'
    	ELSE round(det.nitrogeno::numeric,2)::double precision
    	END) nitrogeno,

    	(CASE WHEN det.ph isnull or det.ph = -1999.91 THEN '0'
    	ELSE round(det.ph::numeric,2)::double precision
    	END) ph,
    	det.idespecietalla,
    	bd.viaje,
    	(
    	  select cm.abreviatura from calidad_observaciones_muestra_cuba cm
    	  where cm.idcalidadobservacion = det.ojos
    	) as ojos,
    	(
    	  select cm.abreviatura from calidad_observaciones_muestra_cuba cm
    	  where cm.idcalidadobservacion = det.agalla
    	) as Agallas,
    	(
    	  select cm.abreviatura from calidad_observaciones_muestra_cuba cm
    	  where cm.idcalidadobservacion = det.piel
    	) as Piel,
    	(
    	  select cm.abreviatura from calidad_observaciones_muestra_cuba cm
    	  where cm.idcalidadobservacion = det.danofisico
    	) as Dano,
    	(
    	  select cm.abreviatura from calidad_observaciones_muestra_cuba cm
    	  where cm.idcalidadobservacion = det.idcalidadolormuestra
    	) as olor,
    	(
    	  select cm.abreviatura from calidad_observaciones_muestra_cuba cm
    	  where cm.idcalidadobservacion = det.idcalidadsabormuestra
    	) as sabor,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'X'
    				else
    				'OK'
    			end
    		else
    			case when det.temperatura <= cobmc.valordesde or det.temperatura = -1999.91 then
    				'OK'
    				else
    				'X'
    			end
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = 57) salida,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'X'
    				else
    				'OK'
    			end
    		else
    			case when det.sal = -1999.91 or (det.sal >= cobmc.valordesde and det.sal <= cobmc.valorhasta) then
    				'OK'
    				else
    				'X'
    			end
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = 53) salidasal,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'X'
    				else
    				'OK'
    			end
    		else
    			case when det.histamina = -1999.91 or (round((det.histamina/10)::numeric,3) >= cobmc.valordesde and round((det.histamina/10)::numeric,3) <= cobmc.valorhasta) then
    				'OK'
    				else
    				'X'
    			end
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = 54) salidahistamina,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'X'
    				else
    				'OK'
    			end
    		else
    			case when det.ph = -1999.91 or (det.ph >= cobmc.valordesde and det.ph <= cobmc.valorhasta) then
    				'OK'
    				else
    				'X'
    			end
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = 55) salidaph,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'X'
    				else
    				'OK'
    			end
    		else
    			case when round(det.nitrogeno::numeric,2) = -1999.91 or (round(det.nitrogeno::numeric,2) <= cobmc.valordesde) then
    				'OK'
    				else
    				'X'
    			end
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = 56) salidanitrogeno,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'OK'
    				else
    				'X'
    			end
    		else
    				'N/A'
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = det.ojos) salidaojos,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'OK'
    				else
    				'X'
    			end
    		else
    				'N/A'
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = det.agalla) salidaagalla,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'OK'
    				else
    				'X'
    			end
    		else
    				'N/A'
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = det.piel) salidapiel,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'OK'
    				else
    				'X'
    			end
    		else
    				'N/A'
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = det.danofisico) salidadano,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'OK'
    				else
    				'X'
    			end
    		else
    				'N/A'
    		end salida
    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = det.idcalidadolormuestra) salidaolor,
    	(select	case when cobmc.idcalidadtipoobservacion = 1 then
    			case when cobmc.calificacion = 1 then
    				'OK'
    				else
    				'X'
    			end
    		else
    				'N/A'
    		end salida

    	from calidad_observaciones_muestra_cuba  cobmc
    	where cobmc.idcalidadobservacion = det.idcalidadsabormuestra) salidasabor, tal.codigoqm as codigoqm


    	from calidad_pcc as cp left join calidad_detalle_pcc as det on cp.idcalidadpcc = det.idcalidadpcc,
    	barco_descarga bd, barco b,calidad_numero_muestreo_pcc as mues,  barco_estiba_cuba bec,
    	barco_cuba bc, cuba cu, especie_talla et, especie as es, talla as tal
    	where cp.idbarcodescarga =  bd.idbarcodescarga and bd.idbarco = b.idbarco
    	and det.nummuestreo = mues.idcalidadnumeromuestreopcc
    	and det.idbarcoestibacuba = bec.idbarcoestibacuba and bec.idbarcocuba = bc.idbarcocuba
    	and bc.idcuba = cu.idcuba and det.idespecietalla = et.idespecietalla
    	and et.idespecie = es.idespecie and et.idtalla = tal.idtalla

    	 and cp.idcalidadpcc = $P{idcalidad}
    	order by cu.idcuba, mues.idcalidadnumeromuestreopcc, det.idespecietalla, det.iddetallecalidadpcc]]>
	</queryString>
