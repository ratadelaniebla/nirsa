	<queryString>
		<![CDATA[select distinct (pdl.idlote) idlote,

(
select min (ddp1.horaingreso)
from descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
) desc_ingreso_area,

(
select max (ddp1.horafin)
from descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
) desc_fin,

mescla_haccp_por_idproducciondetallelote(pdl.idproducciondetallelote) lotehaccp,
b.nombre barco,
(
select
(array_to_string(array_agg(especietalla), '  ' )) especie_talla
from
(
select (e1.codigoqm || ' ' || t1.nombre) especietalla
from produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1,camara_cajon cc1, especie_talla et1, especie e1, talla t1
where
pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdlc1.idcamaracajon = cc1.idcamaracajon
and cc1.idmaterial = et1.idespecietalla
and et1.idespecie = e1.idespecie
and et1.idtalla = t1.idtalla
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
group by et1.idespecietalla, e1.codigoqm, t1.nombre)
 t1
) especie_talla,

pdl.idproducciondetallelote, p.fechaproduccion,
(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (0::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (0::text||' hour')::interval)
) horamon1,
(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (2::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (2::text||' hour')::interval)
) horamon2,
(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (4::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (4::text||' hour')::interval)
) horamon3,
(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (6::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (6::text||' hour')::interval)
) horamon4,

(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (8::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (8::text||' hour')::interval)
) horamon5,
(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (10::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (10::text||' hour')::interval)
) horamon6,
(
select max (ddpt1.fechareg)
from descongelado_detalle_proceso_temp_ing ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
and ddpt1.fechareg between ((ddp1.horainicio + (12::text||' hour')::interval)/*monitoreohfin*/ - (2::text||' hour')::interval) and
(ddp1.horainicio + (12::text||' hour')::interval)
) horamon7,

(
select max(ddpt1.fechareg)
from descongelado_detalle_proceso_temperatura ddpt1, descongelado_detalle_proceso ddp1,
produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
ddpt1.iddescongeladodetalleproceso = ddp1.iddescongeladodetalleproceso
and ddp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
) horatempvolteo,

(
select max(edmtc1.fechareg)
from eviscerado_detalle_muestreo_temp_coche edmtc1,  eviscerado_detalle_proceso_coche edpc1,
eviscerado_detalle_proceso edp1, produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
edmtc1.idevisceradodetalleprocesocoche = edpc1.idevisceradodetalleprocesocoche
and edpc1.idevisceradodetalleproceso = edp1.idevisceradodetalleproceso
and edp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
)horatempevisc,

(
select max(edmtc1.fechareg)
from coccion_detalle_muestreo_temp_coche_ingreso edmtc1,  eviscerado_detalle_proceso_coche edpc1,
eviscerado_detalle_proceso edp1, produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
edmtc1.idevisceradodetalleprocesocoche = edpc1.idevisceradodetalleprocesocoche
and edpc1.idevisceradodetalleproceso = edp1.idevisceradodetalleproceso
and edp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
)horatempingcoc,

(
select max(edmtc1.fechareg)
from coccion_detalle_muestreo_temp_coche_ingreso edmtc1,  eviscerado_detalle_proceso_coche edpc1, cocina_apertura_cierre_detalle cacd1
where
edmtc1.idevisceradodetalleprocesocoche = edpc1.idevisceradodetalleprocesocoche
and edpc1.idcocinaaperturacierredetalle = cacd1.idcocinaaperturacierredetalle
and cacd1.idcocinaaperturacierredetalle =
(
select t1.idcocinaaperturacierredetalle
from
(
select cacd1.idcocinaaperturacierredetalle,
(
select count (edpc2.idevisceradodetalleprocesocoche)
from eviscerado_detalle_proceso_coche edpc2, cocina_apertura_cierre_detalle cacd2
where
edpc2.idcocinaaperturacierredetalle = cacd2.idcocinaaperturacierredetalle
and cacd2.idcocinaaperturacierredetalle = cacd1.idcocinaaperturacierredetalle
) conteo
from eviscerado_detalle_proceso_coche edpc1, cocina_apertura_cierre_detalle cacd1,
eviscerado_detalle_proceso edp1, produccion_detalle_lote_cajon pdlc1, produccion_detalle_lote pdl1
where
edpc1.idcocinaaperturacierredetalle = cacd1.idcocinaaperturacierredetalle
and edpc1.idevisceradodetalleproceso = edp1.idevisceradodetalleproceso
and edp1.idproducciondetallelotecajon = pdlc1.idproducciondetallelotecajon
and pdlc1.idproducciondetallelote = pdl1.idproducciondetallelote
and pdl1.idproducciondetallelote = pdl.idproducciondetallelote
)
t1
order by conteo desc
limit 1
)
)horatempingcoc


from descongelado_detalle_proceso ddp, produccion_detalle_lote_cajon pdlc, camara_cajon cc, produccion_detalle_lote pdl, produccion p,
especie_talla et, especie e, talla t, barco_descarga bd, barco b
where
ddp.idproducciondetallelotecajon = pdlc.idproducciondetallelotecajon
and pdlc.idcamaracajon = cc.idcamaracajon
and pdlc.idproducciondetallelote = pdl.idproducciondetallelote
and pdl.idproduccion = p.idproduccion
and cc.idmaterial = et.idespecietalla
and et.idespecie = e.idespecie
and et.idtalla = t.idtalla
and cc.idbarcodescarga = bd.idbarcodescarga
and bd.idbarco = b.idbarco
and p.idproduccion = $P{IDPRODUCCION}
order by pdl.idlote]]>
	</queryString>
