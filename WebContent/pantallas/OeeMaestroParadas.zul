						<label value="LISTA DE PARADAS"></label>
						<listbox id="lbxListaParadas"  mold="select" width="99%">
				    	</listbox>
					</row>				
				</rows>
			</grid>
			
			
			<grid width="99%">
				<rows>
					<row>
						<label value="DescripciÃ³n"></label>
						<textbox id="txtDescripcion" width="99%" rows="2"></textbox>
					</row>

				</rows>
			</grid>
			


			<label value="CLASES DE PARADAS"></label>
			<grid width="99%">
				<rows>
	
	
					<row>
						<label value="Tiempo muerto"></label>
						<listbox id="lbxTiempoMuerto" style="font-size:11px" mold="select" width="99%">
