						</toolbar>
					</vbox>
				</row>
			</rows>
		</grid>		
	
		<grid width="99%">
			<rows>
				<row>
					<label value="LÃ­nea" ></label>
					<listbox id="lbxLinea" style="font-size:11px" mold="select"> 
						<listitem label="" > </listitem>
						<listitem label="1" > </listitem>
						<listitem label="2" > </listitem>
						<listitem label="3" > </listitem>
						<listitem label="4" > </listitem> 
