		<grid>
			<rows >
				<row>
					<label value="Lote"></label>
					<div>
						<hbox>					
							<listbox id="lbxLote" style="font-size:11px" mold="select" width="80px" ></listbox>
							<toolbar>
								<toolbarbutton id="btnActualizar" tooltiptext="Grabar" image="/img/refrescar.gif"/>
							</toolbar>
						</hbox>
					</div>
				</row>
  			
				<row >
