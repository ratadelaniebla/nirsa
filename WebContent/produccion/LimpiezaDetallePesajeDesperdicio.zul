					<decimalbox id="txtCapturaPeso" format="####"  maxlength="4" width="80px" readonly="false"/>
				</row>

				<row>

					<decimalbox id="txtCapturaPesoLomo" format="####"  maxlength="4" width="1px" readonly="false"/>
