				<row>
				
				<label value=""></label>
				   <toolbar>
							<toolbarbutton id="btnGrabar" label="Grabar" tooltiptext="Grabar" image="/img/grabar.gif"/>
					</toolbar>
				</row>
			</rows>
		</grid>
		



		<label value="RESUMEN LOTE"></label>

		<grid width="99%">
			<rows>


			    <row>
					<label value="ProducciÃ³n " style="color:blue" />
