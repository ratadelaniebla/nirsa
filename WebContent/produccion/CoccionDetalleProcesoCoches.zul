			    </row>

				<row >
					<label value="Lote "></label>
					<hbox>
						<textbox id="txtLoteu" width="70%" > </textbox>
						
						<toolbar>
							<hbox>
								<toolbarbutton id="btnBuscar" label="BUSCAR" />
