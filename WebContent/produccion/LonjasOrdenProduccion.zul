					</row>
					
					<row>
					   <label value="Tipo Empaque:" style="color:blue" />
				       <textbox id="txtEmpaque"   ></textbox>
					</row>
					
				   
			   </rows>
			</grid>  	
			
		   <grid width="99%">
			<rows>
				<row>
					<vbox width="100%" >
						<toolbar>
							<toolbarbutton id="btnGrabar" label="GRABAR" width="320px" />
						</toolbar>
					</vbox>
				</row>
			</rows>
		   </grid>	
		 
	</vbox> 
</window>
</zk>